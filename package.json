{
  "name": "react-tutorial",
  "version": "1.0.0",
  "description": "A react tutorial repository",
  "main": "index.js",
  "scripts": {
    "start": "npm run build",
    "build": "webpack -d && cp src/index.html dist/index.html && webpack-dev-server --content-base src/ --inline --hot --port 8000",
    "build:prod": "webpack -p && cp src/index.html dist/index.html"
  },
  "keywords": [
    "react"
  ],
  "author": "Mike Medina",
  "license": "ISC",
  "dependencies": {
    "react": "^16.8.1",
    "react-dom": "^16.8.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.3.1",
    "@babel/core": "^7.2.2",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "babel-preset-stage-0": "^6.24.1",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14"
  }
}
